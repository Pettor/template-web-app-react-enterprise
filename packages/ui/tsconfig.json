{
  "extends": "@config/typescript/react.json",
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "baseUrl": "src",
    "module": "Preserve",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "paths": {
      ".": ["."]
    },
    "types": ["@config/typescript/types"]
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
