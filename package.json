{
  "name": "template-web-app-react",
  "version": "0.6.0",
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=18.0.0 <23.0.0"
  },
  "scripts": {
    "prepare": "husky",
    "generate:exports": "turbo run generate:exports",
    "clean": "pnpm clean:root && turbo run clean",
    "clean:all": "pnpm clean:root && turbo run clean:all",
    "clean:root": "rimraf output",
    "dev": "turbo run dev --no-daemon --concurrency 20",
    "dev:mocks": "turbo run dev:mocks --no-daemon --concurrency 20",
    "dev:mocks:cli": "pnpm --filter=mock mocks",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint --no-daemon",
    "lint:fix": "turbo run lint:fix --no-daemon",
    "build": "turbo run build --no-daemon",
    "deploy": "turbo run deploy",
    "storybook": "turbo run storybook",
    "storybook:build": "turbo run storybook:build",
    "test": "turbo run test --parallel --no-daemon",
    "test:coverage": "turbo run test:coverage --parallel --no-daemon",
    "test:coverage:ci": "turbo run test:coverage:ci --parallel --no-daemon",
    "test:e2e": "concurrently -k -s \"command-e2e:headless\" -n \"e2e:headless,e2e:env\" -c \"magenta,blue\" \"pnpm test:e2e:headless\" \"pnpm test:e2e:env\"",
    "test:e2e:headless": "pnpm wait-on --timeout 30s tcp:5173 && pnpm --filter=e2e test:e2e",
    "test:e2e:env": "turbo run test:e2e:env --parallel --no-daemon"
  },
  "dependencies": {
    "concurrently": "9.1.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "wait-on": "8.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@commitlint/types": "19.8.1",
    "@mocks-server/main": "4.1.0",
    "@tailwindcss/vite": "4.1.10",
    "@types/node": "22.15.31",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.5.0",
    "@vitest/browser": "3.2.3",
    "@vitest/coverage-v8": "3.2.3",
    "barrelsby": "2.8.1",
    "eslint": "9.27.0",
    "http-server": "14.1.1",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "move-cli": "2.0.0",
    "playwright": "1.52.0",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "0.6.11",
    "rimraf": "6.0.1",
    "tailwindcss": "4.1.10",
    "turbo": "2.5.4",
    "typescript": "5.8.3",
    "usehooks-ts": "3.1.1",
    "utility-types": "3.11.0",
    "vite": "6.3.5",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "pnpm": {
    "overrides": {
      "typescript": "5.8.3",
      "vite": "6.3.5"
    },
    "onlyBuiltDependencies": [
      "@heroui/shared-utils"
    ]
  },
  "lint-staged": {
    "*.js": "prettier --write",
    "*.ts": "prettier --write",
    "*.tsx": "prettier --write",
    "*.scss": "prettier --write"
  }
}
