{
  "name": "@app/storybook",
  "license": "MIT",
  "private": true,
  "type": "module",
  "version": "0.6.0",
  "scripts": {
    "clean": "rimraf storybook-static coverage",
    "clean:all": "pnpm clean node_modules",
    "deploy": "pnpm deploy:build && pnpm deploy:move",
    "deploy:build": "pnpm storybook:build",
    "deploy:move": "move-cli storybook-static \"../../output/deployment/storybook\" --mkdirp",
    "test": "vitest --project=storybook",
    "test:coverage": "vitest --project=storybook --coverage",
    "test:coverage:ci": "pnpm test:coverage && pnpm test:coverage:ci:move",
    "test:coverage:ci:move": "move-cli ./coverage/cobertura-coverage.xml \"../../output/coverage/web.xml\" --mkdirp",
    "storybook": "storybook dev -p 9050",
    "storybook:build": "storybook build -o storybook-static"
  },
  "dependencies": {
    "@app/web": "workspace:*",
    "@config/tailwind": "workspace:*",
    "@fontsource/rubik": "catalog:common",
    "@package/react": "workspace:*",
    "@package/ui": "workspace:*",
    "react": "catalog:react19",
    "react-dom": "catalog:react19",
    "react-intl": "7.1.11"
  },
  "devDependencies": {
    "@config/eslint": "workspace:*",
    "@config/typescript": "workspace:*",
    "@config/vite": "workspace:*",
    "@package/storybook": "workspace:*",
    "@storybook/addon-themes": "catalog:storybook",
    "@storybook/addon-vitest": "catalog:storybook",
    "@storybook/builder-vite": "catalog:storybook",
    "@storybook/react": "catalog:storybook",
    "@storybook/react-vite": "catalog:storybook",
    "@tailwindcss/vite": "catalog:tailwind",
    "@vitejs/plugin-react": "catalog:vite",
    "@vitest/browser": "catalog:vitest",
    "@vitest/coverage-v8": "catalog:vitest",
    "storybook": "catalog:storybook",
    "tailwindcss": "catalog:tailwind",
    "vite": "catalog:vite",
    "vite-tsconfig-paths": "catalog:vite",
    "vitest": "catalog:vitest"
  }
}
