{
  "name": "web",
  "license": "MIT",
  "private": true,
  "type": "module",
  "version": "0.6.0",
  "scripts": {
    "clean": "rimraf dist",
    "clean:all": "pnpm clean node_modules",
    "dev": "vite --open",
    "dev:mocks": "vite --mode mocks",
    "lint": "eslint ./src",
    "lint:fix": "pnpm lint --fix",
    "build": "tsc && vite build",
    "deploy": "pnpm deploy:build && pnpm deploy:move",
    "deploy:build": "pnpm build",
    "deploy:move": "move-cli dist \"../../output/deployment/app\" --mkdirp",
    "test:e2e:env": "pnpm dev:mocks"
  },
  "dependencies": {
    "@fontsource/rubik": "5.2.5",
    "@heroicons/react": "2.2.0",
    "@heroui/react": "2.8.0-beta.6",
    "@hookform/resolvers": "5.0.1",
    "@tanstack/react-query": "5.77.0",
    "api-package": "workspace:*",
    "clsx": "2.1.1",
    "immer": "10.1.1",
    "jotai": "2.12.4",
    "jotai-effect": "2.0.2",
    "react-hook-form": "7.56.4",
    "react-intl": "7.1.11",
    "react-router-dom": "7.6.0",
    "react-package": "workspace:*",
    "ui-package": "workspace:*",
    "tailwind-config": "workspace:*",
    "yup": "1.6.1"
  },
  "devDependencies": {
    "@formatjs/cli": "6.7.1",
    "@storybook/react": "catalog:storybook",
    "@vitejs/plugin-react": "4.5.0",
    "babel-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-config": "workspace:*",
    "storybook": "catalog:storybook",
    "storybook-package": "workspace:*",
    "tailwindcss": "4.1.10",
    "typescript-config": "workspace:*",
    "usehooks-ts": "3.1.1",
    "vite": "6.3.5",
    "vite-plugin-http2-proxy": "0.5.0",
    "vite-plugin-mkcert": "1.17.8",
    "vite-plugin-pwa": "1.0.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3",
    "workbox-window": "7.3.0"
  },
  "jest-junit": {
    "suiteName": "web tests",
    "outputDirectory": ".junit",
    "outputName": "web.xml"
  }
}
